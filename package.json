		"@sveltejs/kit": "^2.0.0",
		"@sveltejs/vite-plugin-svelte": "^3.0.0",
		"@tailwindcss/typography": "^0.5.10",
		"@threlte/core": "^6.0.8",
		"@threlte/extras": "^5.7.0",
		"@threlte/rapier": "^1.1.2",
		"@types/d3": "^7.4.1",
