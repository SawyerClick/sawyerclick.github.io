{
	"name": "sawyerclick.github.io",
	"version": "0.0.1",
	"type": "module",
	"contributors": [
		"Sawyer Click"
	],
	"engines": {
		"node": ">= 16.10.0"
	},
	"browserslist": [
		"defaults and supports es6-module and supports es6-module-dynamic-import, not opera > 0, not samsung > 0, not and_qq > 0"
	],
	"scripts": {
		"dev": "vite dev --host",
		"build": "vite build",
		"preview": "vite preview --host",
		"thumbnails": "node scripts/thumbs.js",
		"preinstall": "npx only-allow pnpm",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"lint": "prettier --plugin-search-dir . --check . && eslint .",
		"format": "prettier --plugin-search-dir . --write ."
	},
	"devDependencies": {
		"@fontsource/esteban": "^5.0.5",
		"@rollup/plugin-dsv": "^3.0.2",
		"@rollup/plugin-yaml": "^4.1.1",
		"@sveltejs/adapter-netlify": "^2.0.8",
		"@sveltejs/kit": "^1.22.3",
		"@tailwindcss/nesting": "0.0.0-insiders.565cd3e",
		"@tailwindcss/typography": "^0.5.9",
		"@types/d3": "^7.4.0",
		"@types/lodash.debounce": "^4.0.7",
		"@types/markdown-it": "^12.2.3",
		"@types/markdown-it-attrs": "^4.1.0",
		"@types/markdown-it-container": "^2.0.5",
		"@typescript-eslint/eslint-plugin": "^5.62.0",
		"@typescript-eslint/parser": "^5.62.0",
		"autoprefixer": "^10.4.14",
		"cssnano": "^6.0.1",
		"d3": "^7.8.5",
		"eslint": "^8.45.0",
		"eslint-config-prettier": "^8.8.0",
		"eslint-plugin-svelte3": "^4.0.0",
		"gsap": "npm:@gsap/shockingly@^3.12.2",
		"journalize": "^2.6.0",
		"js-yaml": "^4.1.0",
		"lodash.debounce": "^4.0.8",
		"markdown-it": "^13.0.1",
		"markdown-it-anchor": "^8.6.7",
		"markdown-it-attrs": "^4.1.6",
		"markdown-it-bracketed-spans": "^1.0.1",
		"markdown-it-container": "^3.0.0",
		"mdsvex": "^0.10.6",
		"postcss": "^8.4.26",
		"postcss-import": "^15.1.0",
		"postcss-load-config": "^4.0.1",
		"prettier": "^2.8.8",
		"prettier-plugin-svelte": "^2.10.1",
		"sharp": "^0.32.3",
		"slugify": "^1.6.6",
		"svelte": "^3.59.2",
		"svelte-check": "^3.4.6",
		"svelte-device-info": "^1.0.0",
		"svelte-feather-icons": "^4.0.1",
		"svelte-preprocess": "^5.0.4",
		"tailwindcss": "^3.3.3",
		"tslib": "^2.6.0",
		"typescript": "^5.1.6",
		"vite-imagetools": "^5.0.5"
	},
	"dependencies": {
		"shelljs": "^0.8.5",
		"vite": "^4.4.4"
	}
}
