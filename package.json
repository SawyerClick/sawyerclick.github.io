		"@types/three": "^0.156.0",
		"@typescript-eslint/eslint-plugin": "^5.62.0",
		"@typescript-eslint/parser": "^5.62.0",
		"autoprefixer": "^10.4.16",
		"cssnano": "^6.0.1",
		"d3": "^7.8.5",
		"eslint": "^8.50.0",
		"eslint-config-prettier": "^8.10.0",
		"eslint-plugin-svelte": "^2.33.2",
		"journalize": "^2.6.0",
		"js-yaml": "^4.1.0",
		"lodash.debounce": "^4.0.8",
		"markdown-it": "^13.0.2",
		"markdown-it-anchor": "^8.6.7",
